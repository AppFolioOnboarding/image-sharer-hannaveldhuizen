<h1>Images Tagged With: <%= @current_tag %></h1>
<table>
  <% @images.each do |image| %>
    <tr>
      <td><%= image.id %></td>
    </tr>
    <tr>
      <td><%= image_tag image.url %>
        <h3>Tags:
          <% image.tag_list.each do |tag| %>
            <a class='btn btn-light' href='<%= tags_path(tag) %>'><%= tag %></a>
          <% end %>
        </h3>
      </td>
    </tr>
  <% end %>
</table>
